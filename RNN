   "id": "4dd425fc-b036-4dc6-bf1e-141244ce9d82",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Original sequence: rtthatpa\n",
      "Predicted characters: rtthatpant\n"
     ]
    }
   ],
   "source": [
    "# testing the prediction result \n",
    "def predict_next_chars(model, tokenizer, text_sequence, num_chars=2):\n",
    "    text_sequence = text_sequence.replace(' ', '')  # Add this line to remove spaces\n",
    "    for _ in range(num_chars):\n",
    "        sequence = tokenizer.texts_to_sequences([text_sequence])[0]\n",
    "        sequence = pad_sequences([sequence], maxlen=char_train_input.shape[1], padding='pre')\n",
    "        prediction = model.predict(sequence, verbose=0)\n",
    "        predicted_index = np.argmax(prediction, axis=1)[0]\n",
    "        predicted_char = tokenizer.index_word[predicted_index]\n",
    "        # Skip spaces if predicted_char is a space\n",
    "        if predicted_char != ' ':\n",
    "            text_sequence += predicted_char\n",
    "    return text_sequence\n",
    "\n",
    "# use RNN to predict the next two character, randomly choose one input sequence from the testing set\n",
    "index = np.random.randint(0, len(char_test_input))\n",
    "sequence = char_tokenizer.sequences_to_texts([char_test_input[index]])[0]\n",
    "# Remove spaces for displaying the original sequence\n",
    "sequence_display = ''.join(sequence.split())\n",
    "predicted_chars = predict_next_chars(char_model, char_tokenizer, sequence)\n",
    "print('Original sequence:', sequence_display)\n",
    "print('Predicted characters:', predicted_chars)\n"
   ]
  },
