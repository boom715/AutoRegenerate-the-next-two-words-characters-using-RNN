    "# define a function to evaluates on the given model on the test data\n",
    "def evaluate_model(model, test_input, test_target, enpochs):\n",
    "    # Evaluate the model on the test data\n",
    "    results = model.evaluate(test_input, test_target, verbose=0)\n",
    "    test_loss = results[0]\n",
    "    test_accuracy = results[1]  \n",
    "    \n",
    "    # Print the results\n",
    "    print(f\"{enpochs} - Test loss:\", test_loss)\n",
    "    print(f\"{enpochs} - Test accuracy:\", test_accuracy)\n",
    "    \n",
    "# evaluate the model after corresponding epochs, epochs 10 \n",
    "evaluate_model(word_model, word_test_input, word_test_target, \"10 enpochs\")"
   ]
